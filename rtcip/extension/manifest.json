{
    "manifest_version": 3,
    "name": "Chrome RTC Proxy IP Logger",
    "version": "1.0",
    "description": "Sets up a local RTC proxy and logs IP address",
    "permissions": [
      "webNavigation"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }